{
	"name": "execute_flask_api",
	"properties": {
		"activities": [
			{
				"name": "customer_testdata",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "post the updated data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": "https://bankclv.azurewebsites.net/api/customers"
				}
			},
			{
				"name": "account_testdata",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "post the updated data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": "https://bankclv.azurewebsites.net/api/accounts"
				}
			},
			{
				"name": "transactions_testdata",
				"type": "WebActivity",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [
					{
						"activity": "post the updated data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": "https://bankclv.azurewebsites.net/api/transactions"
				}
			},
			{
				"name": "post the updated data",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "POST",
					"url": "https://bankclv.azurewebsites.net/api/data"
				}
			},
			{
				"name": "load customer data",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "customer_testdata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzurePostgreSQLSink",
						"writeBatchSize": 1000000,
						"writeBatchTimeout": "00:30:00",
						"writeMethod": "CopyCommand"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['Address']"
								},
								"sink": {
									"name": "cust_address",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['Age']"
								},
								"sink": {
									"name": "cust_age",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['CustomerID']"
								},
								"sink": {
									"name": "cust_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['CustomerName']"
								},
								"sink": {
									"name": "cust_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['Gender']"
								},
								"sink": {
									"name": "cust_gen",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['Income']"
								},
								"sink": {
									"name": "cust_income",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "['JoinDate']"
								},
								"sink": {
									"name": "cust_join_date",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['metadata']['generated_at']"
								},
								"sink": {
									"name": "create_ts",
									"type": "DateTime"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "customer_restsource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "bankingclv_oltp_customer_db",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "load account data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzurePostgreSQLSink",
						"writeBatchSize": 1000000,
						"writeBatchTimeout": "00:30:00",
						"writeMethod": "CopyCommand"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['AccountBalance']"
								},
								"sink": {
									"name": "accnt_bal",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "['AccountID']"
								},
								"sink": {
									"name": "accnt_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['AccountType']"
								},
								"sink": {
									"name": "accnt_type",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['CloseDate']"
								},
								"sink": {
									"name": "accnt_close_dt",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['CustomerID']"
								},
								"sink": {
									"name": "cust_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['OpenDate']"
								}
							},
							{
								"source": {
									"path": "$['metadata']['generated_at']"
								},
								"sink": {
									"name": "create_ts",
									"type": "DateTime"
								}
							}
						],
						"collectionReference": "$['data']"
					}
				},
				"inputs": [
					{
						"referenceName": "account_restsouce",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "bankingclv_oltp_account_db",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}